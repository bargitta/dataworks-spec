    "projectCode" : 10901075322912,
    "userId" : 6,
    "taskType" : "DLC",
    "taskParams" : {"localParams":[],"resourceList":[],"type":"DLC","datasource":7,"sql":"INSERT OVERWRITE TABLE da.dws_dynamics_metric_df PARTITION(ymd='$[yyyy-MM-dd-1]')\nSELECT post_id\n,SUM(like_count) AS like_count\n,SUM(reply_count) AS reply_count\n,SUM(detail_stay_time) AS detail_stay_time\nFROM(\n    SELECT post_id\n    ,SUM(IF(event_name='post_like' AND page IN ('post_page', 'me_page', 'personal_secondary_page', 'post_detail_page') \n                                                                            AND clk_type='1' AND post_type='comment', 1, 0)) AS like_count\n    ,SUM(IF(event_name IN ('comment_result') AND page='post_detail_page' AND result='success' AND post_type='comment', 1, 0)) AS reply_count\n    ,SUM(IF(event_name IN ('page_duration') AND page='post_detail_page',                                        duration, 0)) AS detail_stay_time\n    FROM(\n        SELECT ymd\n        ,page\n        ,event_name\n        ,BIGINT(GET_JSON_OBJECT(properties, '$.post_id')) AS post_id\n        ,GET_JSON_OBJECT(properties, '$.clk_type') AS clk_type\n        ,GET_JSON_OBJECT(properties, '$.post_type') AS post_type\n        ,GET_JSON_OBJECT(properties, '$.result') AS result\n        ,BIGINT(GET_JSON_OBJECT(properties, '$.duration')) AS duration\n        FROM dwd_xingye.xingye_app_log_di\n        WHERE ymd = REPLACE('$[yyyy-MM-dd-1]', '-', '')\n        AND event_name IN ( 'post_view', 'comment_like', 'post_like', 'comment_result','page_duration')\n        AND event_part in ( 'other','page_duration')\n        AND GET_JSON_OBJECT(properties, '$.post_id')>''\n    )t GROUP BY post_id\n\n    UNION ALL\n\n    SELECT post_id\n    ,like_count\n    ,reply_count\n    ,detail_stay_time \n    FROM da.dws_dynamics_metric_df\n    WHERE ymd = '$[yyyy-MM-dd-2]'\n)\nGROUP BY post_id","sqlType":"1","preStatements":[],"postStatements":[],"displayRows":10,"engineType":1,"engineExecType":1,"dataEngineName":"sql-bigdata","dlcTaskType":1,"failureToleranceStrategy":1,"configList":[]},
